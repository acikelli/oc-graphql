{
  "name": "oc-graphql",
  "version": "1.0.3",
  "description": "One Command GraphQL - Serverless GraphQL automation tool for AWS",
  "repository": {
    "type": "git",
    "url": "https://github.com/acikelli/oc-graphql.git"
  },
  "main": "dist/index.js",
  "bin": {
    "oc-graphql": "dist/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "ts-node src/cli.ts",
    "test": "jest",
    "prepare": "npm run build",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write src"
  },
  "keywords": [
    "graphql",
    "serverless",
    "aws",
    "appsync",
    "dynamodb",
    "cli"
  ],
  "author": "Onur Acikelli",
  "license": "MIT",
  "dependencies": {
    "@graphql-tools/schema": "^10.0.0",
    "aws-cdk-lib": "^2.100.0",
    "chalk": "^4.1.2",
    "commander": "^11.0.0",
    "constructs": "^10.3.0",
    "fs-extra": "^11.1.1",
    "graphql": "^16.8.0",
    "graphql-tag": "^2.12.6",
    "ora": "^5.4.1",
    "path": "^0.12.7",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.1",
    "@types/jest": "^29.5.4",
    "@types/node": "^20.5.0",
    "@types/uuid": "^9.0.2",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "@typescript-eslint/parser": "^6.4.0",
    "eslint": "^8.47.0",
    "jest": "^29.6.2",
    "prettier": "^3.0.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  },
  "files": [
    "dist/**/*",
    "README.md"
  ],
  "engines": {
    "node": ">=18.0.0"
  }
}
